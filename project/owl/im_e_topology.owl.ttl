@prefix cim: <https://cim.ucaiug.io/ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix this: <http://data.netbeheernederland.nl/im-e-topology/def#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

this:ACDCTerminal a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ACDCTerminal" ;
    rdfs:subClassOf this:IdentifiedObject ;
    skos:definition "An electrical connection point (AC or DC) to a piece of conducting equipment. Terminals are connected at physical connection points called connectivity nodes." ;
    skos:exactMatch cim:ACDCTerminal ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:BaseVoltage a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "BaseVoltage" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom this:TopologicalNode ;
            owl:onProperty this:topological_node ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:topological_node ] ;
    skos:definition "Defines a system base voltage which is referenced." ;
    skos:exactMatch cim:BaseVoltage ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:ConnectivityNode a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ConnectivityNode" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty this:topological_node ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:topological_node ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:TopologicalNode ;
            owl:onProperty this:topological_node ] ;
    skos:definition "Connectivity nodes are points where terminals of AC conducting equipment are connected together with zero impedance." ;
    skos:exactMatch cim:ConnectivityNode ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:ConnectivityNodeContainer a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ConnectivityNodeContainer" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:topological_node ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:TopologicalNode ;
            owl:onProperty this:topological_node ] ;
    skos:definition "A base class for all objects that may contain connectivity nodes or topological nodes." ;
    skos:exactMatch cim:ConnectivityNodeContainer ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:NameTypeAuthority a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NameTypeAuthority" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty this:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:name_types ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:NameType ;
            owl:onProperty this:name_types ] ;
    skos:definition """Authority responsible for creation and management of names of a given type; typically an organization or an enterprise system.\r
""" ;
    skos:exactMatch cim:NameTypeAuthority ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:Terminal a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Terminal" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:topological_node ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:TopologicalNode ;
            owl:onProperty this:topological_node ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:topological_node ],
        this:ACDCTerminal ;
    skos:definition "An AC electrical connection point to a piece of conducting equipment. Terminals are connected at physical connection points called connectivity nodes." ;
    skos:exactMatch cim:Terminal ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:Name a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Name" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty this:identified_object ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:IdentifiedObject ;
            owl:onProperty this:identified_object ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:NameType ;
            owl:onProperty this:name_type ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty this:name_type ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:identified_object ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:name_type ] ;
    skos:definition """The Name class provides the means to define any number of human readable  names for an object. A name is <b>not</b> to be used for defining inter-object relationships. For inter-object relationships instead use the object identification 'mRID'.\r
""" ;
    skos:exactMatch cim:Name ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:NameType a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NameType" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:name_type_authority ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:NameTypeAuthority ;
            owl:onProperty this:name_type_authority ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty this:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:name_type_authority ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:Name ;
            owl:onProperty this:names ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:names ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:description ] ;
    skos:definition """Type of name. Possible values for attribute 'name' are implementation dependent but standard profiles may specify types. An enterprise may have multiple IT systems each having its own local name for the same object, e.g. a planning system may have different names from an EMS. An object may also have different names within the same IT system, e.g. localName as defined in CIM version 14. The definition from CIM14 is:\r
The localName is a human readable name of the object. It is a free text name local to a node in a naming hierarchy similar to a file directory structure. A power system related naming hierarchy may be: Substation, VoltageLevel, Equipment etc. Children of the same parent in such a hierarchy have names that typically are unique among them.\r
""" ;
    skos:exactMatch cim:NameType ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:connectivity_nodes a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "connectivity_nodes" ;
    skos:definition "The connectivity nodes combine together to form this topological node.  May depend on the current state of switches in the network." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:name_types a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name_types" ;
    skos:definition "All name types managed by this authority." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:terminal a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "terminal" ;
    skos:definition "The terminals associated with the topological node.   This can be used as an alternative to the connectivity node path to terminal, thus making it unnecessary to model connectivity nodes in some cases.   Note that if connectivity nodes are in the model, this association would probably not be used as an input specification." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:IdentifiedObject a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "IdentifiedObject" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty this:short_name ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty this:energy_ident_code_eic ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:energy_ident_code_eic ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:energy_ident_code_eic ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:m_rid ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty this:m_rid ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty this:m_rid ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:names ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:short_name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:short_name ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty this:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:Name ;
            owl:onProperty this:names ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:description ] ;
    skos:definition "This is a root class to provide common identification for all classes needing identification and naming attributes." ;
    skos:exactMatch cim:IdentifiedObject ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:base_voltage a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "base_voltage" ;
    skos:definition "The base voltage of the topological node." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:connectivity_node_container a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "connectivity_node_container" ;
    skos:definition "The connectivity node container to which the topological node belongs." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:energy_ident_code_eic a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "energy_ident_code_eic" ;
    skos:definition "The attribute is used for an exchange of the EIC code (Energy identification Code). The length of the string is 16 characters as defined by the EIC code. For details on EIC scheme please refer to ENTSO-E web site." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:identified_object a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "identified_object" ;
    skos:definition "Identified object that this name designates." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:m_rid a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "m_rid" ;
    skos:definition """Master resource identifier issued by a model authority. The mRID is unique within an exchange context. Global uniqueness is easily achieved by using a UUID, as specified in RFC 4122, for the mRID. The use of UUID is strongly recommended.
For CIMXML data files in RDF syntax conforming to IEC 61970-552, the mRID is mapped to rdf:ID or rdf:about attributes that identify CIM object elements.""" ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:name_type a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name_type" ;
    skos:definition "Type of this name." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:name_type_authority a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name_type_authority" ;
    skos:definition "Authority responsible for managing names of this type." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:short_name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "short_name" ;
    skos:definition "The attribute is used for an exchange of a human readable short name with length of the string 12 characters maximum." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:TopologicalNode a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "TopologicalNode" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom this:ConnectivityNodeContainer ;
            owl:onProperty this:connectivity_node_container ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:Terminal ;
            owl:onProperty this:terminal ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:BaseVoltage ;
            owl:onProperty this:base_voltage ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty this:terminal ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:base_voltage ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty this:base_voltage ],
        [ a owl:Restriction ;
            owl:allValuesFrom this:ConnectivityNode ;
            owl:onProperty this:connectivity_nodes ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty this:connectivity_nodes ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty this:connectivity_node_container ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty this:connectivity_node_container ],
        this:IdentifiedObject ;
    skos:definition """For a detailed substation model a topological node is a set of connectivity nodes that, in the current network state, are connected together through any type of closed switches, including  jumpers. Topological nodes change as the current network state changes (i.e., switches, breakers, etc. change state).
For a planning model, switch statuses are not used to form topological nodes. Instead they are manually created or deleted in a model builder tool. Topological nodes maintained this way are also called "busses".""" ;
    skos:exactMatch cim:TopologicalNode ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:names a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "names" ;
    skos:definition "All names of this identified object.",
        "All names of this type." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    skos:definition """Description of the name type authority.\r
""",
        """Description of the name type.\r
""",
        "The description is a free human readable text describing or naming the object. It may be non unique and may not correlate to a naming hierarchy." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

this:topological_node a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "topological_node" ;
    skos:definition "The topological node associated with the terminal.   This can be used as an alternative to the connectivity node path to topological node, thus making it unnecessary to model connectivity nodes in some cases.   Note that the if connectivity nodes are in the model, this association would probably not be used as an input specification.",
        "The topological node to which this connectivity node is assigned.  May depend on the current state of switches in the network.",
        "The topological nodes at the base voltage.",
        "The topological nodes which belong to this connectivity node container." ;
    skos:inScheme <http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> .

<http://data.netbeheernederland.nl/im-e-topology/version#1.0.0> a owl:Ontology ;
    rdfs:label "TopologyProfile" ;
    dcterms:title "Topology Vocabulary" ;
    skos:definition "This vocabulary describes the topology profile from NBNLES." .

